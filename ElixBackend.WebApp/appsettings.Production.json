{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:8080"
      }
    }
  },
  "JwtSettings": {
    "SecretKey": "JWT_SECRET_PLACEHOLDER",
    "RequireHttpsMetadata": false
  },
  "ConnectionStrings": {
    "DefaultConnection": "DB_CONNECTION_PLACEHOLDER"
  },
  "FileStorage": {
    "UploadsPath": "UPLOADS_PATH_PLACEHOLDER"
  },
  "ApiSettings": {
    "BaseUrl": "https://api.elix.cleanascode.fr"
  }
}